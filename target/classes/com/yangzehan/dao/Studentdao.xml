<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yangzehan.dao.Studentdao">
    <select id="select" resultType="com.yangzehan.domaim.Student">
      select * from student;
  </select>
    <insert id="insert">
        insert into student(name,age)values (#{name},#{age});
    </insert>
</mapper>